{
  "name": "@svrooij/sonos-cli",
  "description": "Control your sonos players right from the console.",
  "version": "0.0.0-development",
  "author": "Stephan van Rooij",
  "bin": {
    "sonos": "./bin/run"
  },
  "bugs": "https://github.com/svrooij/sonos-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "@svrooij/sonos": "^1.2.0",
    "cli-ux": "^5.4.4",
    "fs-extra": "^8.1.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.9",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^8.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.7.1",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "semantic-release": "^15.14.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/svrooij/sonos-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sonos",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "svrooij/sonos-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "semantic-release": "semantic-release"
  },
  "types": "lib/index.d.ts",
  "topics": {
    "alarm": {
      "description": "Manage your alarms"
    }
  }
}
